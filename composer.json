{
    "name": "mawuekom/laravel-searchable",
    "description": "Laravel package to make easy search on Eloquent model",
    "keywords": [
        "mawuekom",
        "laravel-searchable"
    ],
    "homepage": "https://github.com/mawuva/laravel-searchable",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ephra√Øm Seddor",
            "email": "seddorephraim7@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mawuekom\\LaravelSearchable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mawuekom\\LaravelSearchable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mawuekom\\LaravelSearchable\\LaravelSearchableServiceProvider"
            ],
            "aliases": {
                "LaravelSearchable": "Mawuekom\\LaravelSearchable\\LaravelSearchableFacade"
            }
        }
    }
}
